        <name>Convert to Issue</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[if (current) {

    var flowContextGR = new GlideRecord('sys_flow_context');
    flowContextGR.addQuery('source_record', current.sys_id);
    flowContextGR.addQuery('state', 'Waiting');
    flowContextGR.query();
    if (flowContextGR.next()) {
        sn_fd.FlowAPI.cancel(flowContextGR.getUniqueValue(), 'Canceling the flow due to a change in support type');
    }

    var gr = new GlideRecord(current.getTableName());
    if (gr.get(current.sys_id)) {
        gr.support_type = 'Issue';
		gr.approval = 'not requested';

        gr.update();

    }
    action.setRedirectURL(current);
}]]></script>
