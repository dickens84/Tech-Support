        <messages/>
        <name>Tech support - Request for On Change</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    var ga = new GlideAjax('GetUserDetails');
    ga.addParam('sysparm_name', 'getUserPhoneDetails');
    ga.addParam('sys_id', newValue);
    ga.getXMLAnswer(function(response) {

        //  var answer = response.responseXML.documentElement.getAttribute("answer");

        var userObj = JSON.parse(response);

        if (userObj.mobilePhone !== '')
            g_form.setValue('contact_number', userObj.mobilePhone, userObj.mobilePhone);
        else if (userObj.businessPhone !== '')
            g_form.setValue('contact_number', userObj.businessPhone, userObj.businessPhone);
        else
            g_form.setValue('contact_number', '', '');
    });


}]]></script>
